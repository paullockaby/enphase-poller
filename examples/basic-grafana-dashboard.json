          "rawQuery": true,
          "rawSql": "SELECT\n  date_trunc('day', c.time) AS \"Date\",\n  max(c.wh_today)/1000 AS \"kWh Consumed\",\n  max(p.wh_today)/1000 AS \"kWh Produced\",\n  max(p.wh_today)/max(c.wh_today)*100 AS \"Ratio\"\nFROM (\n    SELECT date_trunc('minute', reading_time) AS time, *\n    FROM public.consumption\n    WHERE reading_time - '11 days'::interval < $__timeTo() \n) AS c\nJOIN (\n    SELECT date_trunc('minute', reading_time) AS time, *\n    FROM public.production\n    WHERE reading_time - '11 days'::interval < $__timeTo() \n) AS p ON c.time = p.time\nGROUP by date_trunc('day', c.time)\nORDER by date_trunc('day', c.time) DESC\nLIMIT 30",
