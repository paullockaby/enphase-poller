          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n    c.time AS \"Date\",\n    c.wh_today/1000 AS \"kWh Consumed\",\n    p.wh_today/1000 AS \"kWh Produced\",\n    p.wh_today/c.wh_today*100 AS \"Ratio\"\nFROM (\n    SELECT cp.reading_time AT TIME ZONE 'UTC' AS time, cp.wh_today, cp.wh_lifetime\n    FROM (\n        SELECT reading_time AT TIME ZONE '${timezone}' AS reading_time, wh_today, wh_lifetime\n        FROM public.consumption\n    ) cp\n    WHERE cp.reading_time IN (\n        SELECT max(x.reading_time) FROM (\n            SELECT reading_time AT TIME ZONE '${timezone}' AS reading_time\n            FROM public.consumption\n        ) x\n        GROUP BY date_trunc('day', x.reading_time)\n    )\n) c\nJOIN (\n    SELECT pp.reading_time AT TIME ZONE 'UTC' AS time, pp.wh_today, pp.wh_lifetime\n    FROM (\n        SELECT reading_time AT TIME ZONE '${timezone}' AS reading_time, wh_today, wh_lifetime\n        FROM public.production\n    ) pp\n    WHERE pp.reading_time IN (\n        SELECT max(x.reading_time) FROM (\n            SELECT reading_time AT TIME ZONE '${timezone}' AS reading_time\n            FROM public.production\n        ) x\n        GROUP BY date_trunc('day', x.reading_time)\n    )\n) p ON c.time = p.time\nORDER BY 1 DESC",
          "refId": "A",
