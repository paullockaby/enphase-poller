          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT $__timeGroup(c.time, '1m') AS time,\n    p.w_now/1000 AS \"Produced\",\n    c.w_now/1000 AS \"Consumed\",\n    CASE\n        WHEN c.w_now > p.w_now THEN\n            (c.w_now-p.w_now)/1000\n        ELSE 0\n    END AS \"Imported\",\n    CASE\n        WHEN p.w_now > c.w_now THEN\n            (p.w_now-c.w_now)/1000\n        ELSE 0\n    END AS \"Exported\"\nFROM (\n    SELECT date_trunc('minute', reading_time) AS time, *\n    FROM public.consumption\n    WHERE $__timeFilter(reading_time)\n) AS c\nJOIN (\n    SELECT date_trunc('minute', reading_time) AS time, *\n    FROM public.production\n    WHERE $__timeFilter(reading_time)\n) AS p ON c.time = p.time\n",
          "refId": "A",
